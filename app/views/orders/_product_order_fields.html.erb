<div class="nested-fields">
  <%= f.hidden_field :_destroy %>
  <div>
    <%= f.label :quantitie %>
    <%= f.text_field :quantitie %>
  </div>
  <div>
    <%= f.label :note %>
    <%= f.text_field :note %>
  </div>
  <div>
    <%= f.label :product_id %>
    <%= collection_select(:product_order, :product_id, Product.all, :id, :name,  {prompt: 'Selecione'}, class: "form-control select2") %>
  </div>
  <div>
    <%= link_to "Remove", '#', class: "remove_fields" %>
  </div>
</div>
